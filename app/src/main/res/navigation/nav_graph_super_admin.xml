<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph2"
    app:startDestination="@id/addEmployeeFragment">
    <fragment
        android:id="@+id/addEmployeeFragment"
        android:name="com.smartgig.tech.ui.fragments.AddEmployeeFragment"
        android:label="fragment_add_employee"
        tools:layout="@layout/fragment_add_employee" >
        <action
            android:id="@+id/action_addEmployeeFragment_to_employeeListFragment"
            app:destination="@id/employeeListFragment" />
    </fragment>
    <fragment
        android:id="@+id/employeeListFragment"
        android:name="com.smartgig.tech.ui.fragments.EmployeeListFragment"
        android:label="fragment_employee_list"
        tools:layout="@layout/fragment_employee_list" >
        <action
            android:id="@+id/action_employeeListFragment_to_addEmployeeFragment"
            app:destination="@id/addEmployeeFragment" />
        <action
            android:id="@+id/action_employeeListFragment_to_adminAccessFragment"
            app:destination="@id/adminAccessFragment" />
    </fragment>
    <fragment
        android:id="@+id/adminAccessFragment"
        android:name="com.smartgig.tech.ui.fragments.AdminAccessFragment"
        android:label="fragment_admin_access"
        tools:layout="@layout/fragment_admin_access" >
        <action
            android:id="@+id/action_adminAccessFragment_to_employeeListFragment"
            app:destination="@id/employeeListFragment" />
        <action
            android:id="@+id/action_adminAccessFragment_to_addEmployeeFragment"
            app:destination="@id/addEmployeeFragment" />
    </fragment>
</navigation>